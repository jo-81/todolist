<div{{ attributes.defaults({class: "row g-3"}) }}>

    <p class="mb-0 fw-semibold">Nombre de projets : {{ this.workspaces.totalItemCount }}</p>

    <twig:Bootstrap:Form:Search placeholder="Rechercher un workspace"/>

    {% for workspace in this.workspaces %}
        <twig:Workspace:Card key="workspace-{{ workspace.id }}" workspace="{{ workspace }}"/>
        {% else %}
        <p>Vous n'avez pas encore cr√©er de Workspace</p>
    {% endfor %}

    {{ knp_pagination_render(this.workspaces) }}
</div>
